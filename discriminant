#include <iostream> 
#include <cstdlib> 
using namespace std;

int main()
{
	int a, b, c;
	int d;
	int x1, x2;
	cout << "write a" << endl;
	cin >> a;
	cout << "write b" << endl;
	cin >> b;
	cout << "write c" << endl;
	cin >> c;
	d = b*b - (4 * a*c);
	if (d > 0)
	{
		x1 = (-b - sqrt(d)) / 2 * a;
		x2 = (-b + sqrt(d)) / 2 * a;
		cout << "x1 = ";
		cout << x1 << endl;
		cout << "x2 = ";
		cout << x2 << endl;
	}
	else if (d < 0)
	{
		cout << "No answers";
	}
	else if (d = 0)
	{
	  x1 = (-b - sqrt(d)) / 2 * a;
	  cout << "x = ";
	  cout << x1 << endl;
	}
	system("pause>nul");
	return 0;
}
